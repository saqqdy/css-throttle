{
	"name": "css-throttle",
	"description": "A high-performance solution using css to prevent button double-clicking.",
	"type": "module",
	"version": "1.0.0-beta.1",
	"packageManager": "pnpm@8.6.0",
	"main": "src/css.js",
	"module": "src/css.mjs",
	"style": "src/index.css",
	"exports": {
		".": {
			"require": "./src/css.js",
			"import": "./src/css.mjs"
		},
		"./less": {
			"require": "./src/index.js",
			"import": "./src/index.mjs"
		},
		"./css": {
			"require": "./src/css.js",
			"import": "./src/css.mjs"
		},
		"./scss": {
			"require": "./src/scss.js",
			"import": "./src/scss.mjs"
		},
		"./*": "./*"
	},
	"directories": {
		"dist": "dist",
		"src": "src"
	},
	"files": [
		"dist",
		"typings"
	],
	"scripts": {
		"pub": "node --loader @esbuild-kit/esm-loader scripts/publish",
		"unpub": "node --loader @esbuild-kit/esm-loader scripts/unpublish",
		"sync": "node --loader @esbuild-kit/esm-loader scripts/sync",
		"workflow:publish-test": "zx scripts/workflow.mjs",
		"clean": "rm-all dist es lib",
		"dist": "run-s eslint prettier",
		"eslint": "eslint --fix .",
		"prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,yml,json,md}\""
	},
	"devDependencies": {
		"@esbuild-kit/esm-loader": "^2.5.5",
		"@eslint-sets/eslint-config-basic": "^5.2.0",
		"@node-kit/extra.fs": "^2.5.1",
		"@types/node": "^20.2.1",
		"eslint": "^8.40.0",
		"load-yml": "^1.3.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.8",
		"prettier-config-common": "^1.4.0",
		"reinstaller": "^3.0.0",
		"rm-all": "^1.0.0",
		"typescript": "^5.0.4",
		"zx": "^7.2.2"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"tslib",
				"webpack"
			],
			"allowedVersions": {
				"eslint": "^8.0.0"
			}
		}
	},
	"keywords": [
		"plugin",
		"extension",
		"css-throttle",
		"dbclick",
		"throttle",
		"saqqdy"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"homepage": "https://github.com/saqqdy/css-throttle#readme",
	"bugs": {
		"url": "https://github.com/saqqdy/css-throttle/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/css-throttle.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
}
